# Convex Environment Variables
# Copy this to .env.local after running `npx convex dev`

# Convex Deployment URL (get this from `npx convex dev`)
CONVEX_DEPLOYMENT=

# GitHub OAuth
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# AI Provider Keys (users can also provide their own)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=

# Webhook Configuration
GITHUB_WEBHOOK_SECRET=

# App URLs
NEXT_PUBLIC_APP_URL=http://localhost:5173
BACKEND_URL=http://localhost:3000
